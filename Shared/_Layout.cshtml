                display: flex;
                justify-content: center;
                width: 90%;
                max-width: 800px;
                margin: 0 auto;
            }

            .team .profile {
                position: relative;
                transition: all 0.3s;
            }

                .team .profile:hover {
                    transform: translateY(15px);
                }

                .team .profile img {
                    max-width: 100%;
                    border-radius: 50%;
                    border: 3px solid #f7f7f7;
                    filter: drop-shadow(-10px 0 5px rgba(0, 0, 0, 0.1));
                    cursor: pointer;
                }

                .team .profile:not(:first-child) img {
                    margin-left: -10px;
                }

                .team .profile .name {
                    position: absolute;
                    background-color: #10996d;
                    color: #fff;
                    font-family: "#9Slide06 Ong Do Gia", cursive;
                    padding: 10px 20px;
                    border-radius: 50px;
                    bottom: -40px;
                    left: 50%;
                    white-space: nowrap;
                    transform: translate(-50%, -30px);
                    letter-spacing: 1px;
                    font-size: 16px;
                    opacity: 0;
                    transition: all 0.3s;
                }

                    .team .profile .name::before {
                        content: "";
                        position: absolute;
                        width: 10px;
                        height: 10px;
                        background-color: #10996d;
                        top: 0;
                        left: 50%;
                        transform: translate(-50%, -50%) rotate(45deg);
                    }

                .team .profile img:hover + .name {
                    opacity: 1;
                    transform: translateX(-50%);
                    box-shadow: 0 5px 10px rgba(86, 86, 198, 0.3);
                }
